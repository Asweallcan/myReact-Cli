{
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "extends": ["eslint:recommended"],
  "parser": "babel-eslint",
  "plugins": ["react"],
  "rules": {
    "eqeqeq": "error",
    "semi": "error",
    "no-unused-vars": "off",
    "react/jsx-filename-extension": "off"
  },
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true,
      "modules": true
    }
  }
}
